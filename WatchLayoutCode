#include <stdio.h>

int main() {
	char day[3] = "SU", month[3] = "06", date[3] = "30", hour[3] = "09", minute[3] = "58", sec[] = "03", alarm[3] = " A";

	//Tikekeeping
	printf("        ####################\n");
	printf("     ###                    ###\n");
	printf("  ###      %s       %s/%s      ###\n", day, month, date);
	printf("##                                ##\n");
	printf("##    %s                          ##\n", alarm);
	printf("##          %s : %s . %s          ##\n", hour, minute, sec);
	printf("  ###                          ###\n");
	printf("     ###                    ###\n");
	printf("        ####################\n");

	printf("\n\n");

	char stopwatch[3] = "ST";
	char stopwatchM[3] = "05", stopwatchS[3] = "54", stopwatchMS[3] = "04";

	//Stopwatch
	printf("        ####################\n");
	printf("     ###                    ###\n");
	printf("  ###      %s       %s:%s      ###\n", stopwatch, hour, minute);
	printf("##                                ##\n");
	printf("##    %s                          ##\n", alarm);
	printf("##          %s\'  %s\"  %s          ##\n", stopwatchM, stopwatchS, stopwatchMS);
	printf("  ###                          ###\n");
	printf("     ###                    ###\n");
	printf("        ####################\n");

	printf("\n\n");

	char alarmMode[3] = "AL";
	char alarmH[3] = "12", alarmM[3] = "00";

	//Alarm
	printf("        ####################\n");
	printf("     ###                    ###\n");
	printf("  ###      %s       %s/%s      ###\n", alarmMode, month, date);
	printf("##                                ##\n");
	printf("##    %s                          ##\n", alarm);
	printf("##          %s : %s               ##\n", alarmH, alarmM);
	printf("  ###                          ###\n");
	printf("     ###                    ###\n");
	printf("        ####################\n");

	return 0;
}
